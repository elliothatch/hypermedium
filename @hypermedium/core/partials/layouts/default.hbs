{{#if headline}}<h1>{{headline}}</h1>
{{else if name}}<h1>{{name}}</h1>{{/if}}
{{#if articleBody}}<p>{{articleBody}}</p>
{{else if description}}<p>{{description}}</p>{{/if}}

<div class="container">
	{{#each this}}
	<div>
		{{#unless (or (or (eq @key 'name') (eq @key 'description')) (and (startsWith @key '_') (not (eq @key '_embedded'))))}}
			<h2>{{@key}}</h2>
			{{#if (eq (typeof this) 'object')}}
				{{> includes/json-object.hbs this}}
			{{else}}
				<div>{{this}}</div>
			{{/if}}
		{{/unless}}
	</div>
	{{/each}}
</div>
